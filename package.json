{
  "name": "texscript",
  "version": "0.0.2",
  "main": "index.ts",
  "private": "true",
  "scripts": {
    "build-v1:dev": "npx rimraf .temp/build/v1 && node scripts/v1/dev-bundle.js",
    "build-v1": "npx rimraf build/v1 && node scripts/v1/build-bundle.js",
    "build-v2:dev": "npx rimraf .temp/build/v2 && node scripts/v2/dev-bundle.js",
    "build-v2": "npx rimraf build/v2 build/min/v2 && node scripts/v2/build-bundle.js",
    "build-v3:dev": "npx rimraf .temp/build/v3 && node scripts/v3/dev-bundle.js",
    "build-v3": "npx rimraf build/v3 build/min/v3 && node scripts/v3/build-bundle.js",
    "build": "npm run build-v1 && npm run build-v2 && npm run build-v3",
    "dev-v2": "node scripts/v2/dev.mjs",
    "dev-v3": "node scripts/v3/dev.mjs",
    "preview": "npx http-server build --port=8080",
    "serve-v1": "node servers/v1/src-lib-watcher-builder.js",
    "serve-v2": "node servers/v2/src-lib-watcher-builder.js",
    "serve-v3": "node servers/v3/src-lib-watcher-builder.js",
    "serve": "node servers/app.js",
    "start": "npm run serve",
    "tscgo": "npx @typescript/native-preview"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "devDependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20251013.1",
    "chokidar": "^4.0.3",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.10",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "http-server": "^14.1.1",
    "serve-index": "^1.9.1",
    "strip-comments": "^2.0.1",
    "terser": "^5.44.0",
    "typescript": "^5.9.3"
  }
}
