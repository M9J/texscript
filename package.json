{
  "name": "texscript",
  "version": "0.0.2",
  "main": "index.ts",
  "scripts": {
    "build": "npm run v1-build && npm run v2-build",
    "preview": "npx http-server build --port=80",
    "serve": "node servers/app.js",
    "start": "npm run serve",
    "v1-build:bundle": "npx rimraf build/v1 && node scripts/v1/build-bundle.js",
    "v1-build": "npm run v1-build:bundle",
    "v1-dev:all": "npm run v1-dev:bundle",
    "v1-dev:bundle": "npx rimraf .temp/build/v1 && node scripts/v1/dev-bundle.js",
    "v1-serve": "node servers/v1/src-lib-watcher-builder.js",
    "v2-build:bundle": "npx rimraf build/v2 build/min/v2 && node scripts/v2/build-bundle.js",
    "v2-build": "npm run v2-build:bundle",
    "v2-dev:all": "npm run v2-dev:bundle",
    "v2-dev:bundle": "npx rimraf .temp/build/v2 && node scripts/v2/dev-bundle.js",
    "v2-dev": "node scripts/v2/dev.mjs",
    "v2-serve": "node servers/v2/src-lib-watcher-builder.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "devDependencies": {
    "chokidar": "^4.0.3",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.10",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "http-server": "^14.1.1",
    "serve-index": "^1.9.1",
    "typescript": "^5.9.3"
  }
}
